  },
  "homepage": "https://wire.com",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "GPL-3.0",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "electron/!(renderer)/**/!(*.test*).ts": [
      "tslint --project tsconfig.json --fix",
      "git add"
    ],
    "electron/renderer/**/!(*.test*).{ts,tsx}": [
      "tslint --project tsconfig.renderer.json --fix",
      "git add"
    ],
    "**/*.test*.ts": [
      "tslint --project tsconfig.mocha.json --fix",
      "git add"
    ],
    "*.{json,md,css,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "electron/dist/main.js",
  "name": "wire-desktop",
  "optionalDependencies": {
    "@wireapp/node-addressbook": "3.3.1"
  },
  "engines": {
    "node": ">= 10.9",
    "yarn": ">= 1"
  },
  "prettier": "@wireapp/prettier-config",
  "private": true,
  "productName": "WireInternal",
  "repository": {
    "type": "git",
    "url": "https://github.com/wireapp/wire-desktop.git"
  },
  "scripts": {
    "build:linux": "yarn build:prepare && wire-build-linux -w electron/wire.json",
    "build:linux:internal": "cross-env-shell APP_ENV=internal \"yarn configure && yarn build:linux\"",
    "build:macos": "yarn build:prepare && wire-build-macos -m -w electron/wire.json",
    "build:macos:internal": "cross-env-shell APP_ENV=internal \"yarn configure && yarn build:macos\"",
    "build:prepare": "yarn clear:wrap && yarn build:ts && yarn bundle",
    "build:ts": "yarn clear:ts && tsc",
    "build:ts:tests": "tsc -P tsconfig.mocha.json --noEmit",
    "build:ts:renderer": "tsc -P tsconfig.renderer.json --noEmit",
    "build:win": "yarn build:prepare && wire-build-win -w electron/wire.json",
    "build:win:installer": "rimraf wrap/dist && wire-build-win-installer -w electron/wire.json",
    "build:win:internal": "cross-env-shell APP_ENV=internal \"yarn configure && yarn build:win\"",
    "bundle": "yarn clear:webpack && webpack --mode=production",
    "bundle:watch": "yarn clear:webpack && webpack-dev-server",
    "clear:coverage": "rimraf coverage",
    "clear:ts": "rimraf electron/dist",
    "clear:ts:renderer": "rimraf electron/renderer/dist",
    "clear:wrap": "rimraf wrap",
    "clear:webpack": "rimraf electron/renderer/cache",
    "configure": "copy-config",
    "coverage": "yarn clear:coverage && nyc report",
    "fix": "yarn fix:js && yarn fix:other && yarn fix:ts && yarn fix:ts:renderer && yarn fix:ts:tests",
    "fix:js": "yarn lint:js --fix",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "fix:ts:tests": "yarn lint:ts:tests --fix",
    "fix:ts:renderer": "yarn lint:ts:renderer --fix",
    "lint": "yarn lint:js && yarn lint:other && yarn lint:ts && yarn lint:ts:renderer && yarn lint:ts:tests",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --project tsconfig.json",
    "lint:ts:tests": "tslint --project tsconfig.mocha.json",
    "lint:ts:renderer": "tslint --project tsconfig.renderer.json",
    "postinstall": "yarn configure && yarn rebuild",
    "prettier": "prettier \"**/*.{json,md,css,yml}\"",
    "rebuild": "electron-rebuild -f -w @wireapp/node-addressbook",
    "start": "cross-env NODE_DEBUG=@wireapp* electron . --inspect --devtools --enable-logging",
    "start:dev": "yarn build:ts && yarn start --env=https://wire-webapp-dev.zinfra.io",
    "start:edge": "yarn build:ts && yarn start --env=https://wire-webapp-edge.zinfra.io",
    "start:internal": "yarn build:ts && yarn start --env=https://wire-webapp-staging.wire.com",
    "start:localhost": "yarn build:ts && yarn start --env=http://localhost:8081",
    "start:prod": "yarn build:ts && yarn start --env=https://app.wire.com",
    "start:rc": "yarn build:ts && yarn start --env=https://wire-webapp-rc.zinfra.io",
    "start:watch": "yarn build:ts && concurrently \"yarn bundle:watch\" \"yarn start\"",
    "test": "yarn lint && yarn build:ts && yarn build:ts:renderer && yarn build:ts:tests && yarn test:react && yarn test:main && yarn coverage",
    "test:main": "electron-mocha --require .babel-register.js \"electron/src/**/*.test?(.main).ts\"",
    "test:types": "tsc --noEmit -P tsconfig.renderer.json",
    "test:react": "jest"
  }
}
